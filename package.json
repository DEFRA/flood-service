{
  "name": "flood-service",
  "version": "8.25.0",
  "description": "Flood risk service",
  "engines": {
    "node": "22.x"
  },
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "start:local": "npx nodemon --ext js,json,html,env -r dotenv/config index.js",
    "start:local:no-watch": "node -r dotenv/config index.js",
    "unit-test": "export LOG_LEVEL=silent && lab -v -c -r console -o stdout -r lcov -o coverage/lcov.info --require dotenv/config",
    "unit-test:no-coverage": "export LOG_LEVEL=silent && lab -v -c -r console -o stdout --require dotenv/config",
    "test": "npm run lint && npm run unit-test",
    "create-release-notes": "node release-docs/lib/create-release-notes.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@airbrake/node": "2.1.9",
    "@aws-sdk/client-s3": "3.844.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.3",
    "blipp": "4.0.2",
    "hapi-pino": "13.0.0",
    "install": "0.13.0",
    "joi": "18.0.1",
    "npm": "11.7.0",
    "nunjucks": "3.2.4",
    "pg": "8.16.3",
    "pino": "9.12.0",
    "pino-abstract-transport": "2.0.0",
    "pino-pretty": "13.1.1",
    "standard": "17.1.2",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@hapi/code": "9.0.3",
    "@hapi/lab": "26.0.0",
    "aws-sdk-client-mock": "4.1.0",
    "dotenv": "17.2.3",
    "nodemon": "3.1.10",
    "proxyquire": "2.1.3",
    "sinon": "21.0.0"
  }
}
