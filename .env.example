# Application
NODE_ENV=development
PORT=8050

# Build Args
BUILD_VERSION=v8.23.0
GIT_COMMIT=local
PARENT_VERSION=2.10.3-node22.21.1
PORT_DEBUG=9229

# Database
FLOOD_SERVICE_CONNECTION_STRING=postgresql://postgres:postgres@postgres:5432/floodservice
POSTGRES_DB=floodservice
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# S3 Configuration - Localstack
# Note: In deployed environments (live/non-live), these credentials are not needed
# as the service will use IAM roles. Only required for local development/localstack.
FLOOD_SERVICE_S3_ACCESS_KEY=test
FLOOD_SERVICE_S3_SECRET_ACCESS_KEY=test
FLOOD_SERVICE_S3_REGION=eu-west-2
FLOOD_SERVICE_S3_BUCKET=flood-service-bucket
FLOOD_SERVICE_S3_TIMEOUT=10000
AWS_ENDPOINT_URL=http://localstack:4566

# Logging
LOG_LEVEL=info
DEBUG=0

# Errbit
ERRBIT_ENABLED=false
ERRBIT_HOST=https://errbit-prd.aws-int.defra.cloud
ERRBIT_PROJECT_ID=1
ERRBIT_PROJECT_KEY=

# Optional: Real AWS credentials to sync data to localstack on startup
# Leave empty to use sample data instead
REAL_AWS_ACCESS_KEY_ID=
REAL_AWS_SECRET_ACCESS_KEY=
REAL_S3_BUCKET=
AWS_REGION=eu-west-2
